{% extends 'base.html.twig' %}

{% block title %}Ajouter un Employé{% endblock %}

{% block body %}
<div class="container mt-10">
    <h1>Ajouter un nouvel Employé</h1>

    {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.poste) }}
        {{ form_row(form.statut) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}
    </div>
{% endblock %}
